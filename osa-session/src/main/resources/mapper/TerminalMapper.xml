        <result column="remote_addr" property="remoteAddr" />
        <result column="ssh_port" property="sshPort" />
        <result column="http_port" property="httpPort" />
        <result column="is_accepted" property="isAccepted" />
        <result column="is_deleted" property="isDeleted" />
        <result column="date_created" property="dateCreated" />
        <result column="comment" property="comment" />
        <result column="userguid" property="userguid" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id, name, remote_addr, ssh_port, http_port, is_accepted, is_deleted, date_created, comment, userguid
